<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_borrow" model="ir.sequence">
            <field name="name">Borrowing Sequence</field>
            <field name="code">library.borrow.sequence</field>
            <field name="prefix">PNJM</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_fine_adjustment" model="ir.sequence">
            <field name="name">Fine Adjustment Sequence</field>
            <field name="code">library.fine.adjustment.sequence</field>
            <field name="prefix">FINE-ADJ</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_fine" model="ir.sequence">
            <field name="name">Fine Sequence</field>
            <field name="code">library.fine.sequence</field>
            <field name="prefix">FINE-</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>