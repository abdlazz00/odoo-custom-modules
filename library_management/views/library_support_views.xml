<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Your custom views here -->
    <record id="library_author_list_view" model="ir.ui.view">
        <field name="name">library.author.view.list</field>
        <field name="model">library.author</field>
        <field name="arch" type="xml">
            <list string="Authors" editable="top">
                <field name="name"/>
            </list>
        </field>
    </record>

    <record id="library_author_action" model="ir.actions.act_window">
        <field name="name">Authors</field>
        <field name="res_model">library.author</field>
        <field name="view_mode">list</field>
    </record>

    <record id="library_publisher_view_list" model="ir.ui.view">
        <field name="name">library.publisher.list</field>
        <field name="model">library.publisher</field>
        <field name="arch" type="xml">
            <list string="Publishers" editable="top">
                <field name="name"/>
            </list>
        </field>
    </record>

    <record id="library_publisher_action" model="ir.actions.act_window">
        <field name="name">Publisher</field>
        <field name="res_model">library.publisher</field>
        <field name="view_mode">list</field>
    </record>

    <record id="library_genre_view_list" model="ir.ui.view">
        <field name="name">Genre</field>
        <field name="model">library.genre</field>
        <field name="arch" type="xml">
            <list string="Genre" editable="top">
                <field name="name"/>
            </list>
        </field>
    </record>

    <record id="library_genre_action" model="ir.actions.act_window">
        <field name="name">Book genres</field>
        <field name="res_model">library.genre</field>
        <field name="view_mode">list</field>
    </record>

    <record id="library_category_view_list" model="ir.ui.view">
        <field name="name">library.category.view.list</field>
        <field name="model">library.category</field>
        <field name="arch" type="xml">
            <list string="Category Books" editable="top">
                <field name="name"/>
            </list>
        </field>
    </record>

    <record id="library_category_action" model="ir.actions.act_window">
        <field name="name">Book Categories</field>
        <field name="res_model">library.category</field>
        <field name="view_mode">list</field>
    </record>
</odoo>